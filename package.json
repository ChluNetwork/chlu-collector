{
  "name": "chlu-collector",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.6.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/bin.js",
    "test": "mocha --opts tests/mocha.opts"
  },
  "bin": {
    "chlu-service-node": "src/bin.js"
  },
  "dependencies": {
    "chlu-ipfs-support": "ChluNetwork/chlu-ipfs-support#misc-features",
    "commander": "^2.15.1",
    "libp2p-websocket-star-rendezvous": "^0.2.3",
    "lodash": "^4.17.5"
  },
  "//resolutions": {
    "ipfs-log": "ChluNetwork/ipfs-log#custom-verification",
    "ipfs-pubsub-1on1": "ChluNetwork/ipfs-pubsub-1on1#ipfs-api-fix"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "mocha": "^4.0.1",
    "sinon": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "no-console": 0,
      "no-debugger": 1,
      "no-var": 1,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0
    },
    "globals": {}
  },
  "repository": "git@github.com:ChluNetwork/chlu-collector.git",
  "license": "MIT"
}
